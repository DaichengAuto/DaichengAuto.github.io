body {
    font-family: Arial, sans-serif;
    margin: 0;
    padding: 0;
}

header {
    background-color: #333;
    color: white;
    padding: 10px 20px;
}

nav ul {
    list-style-type: none;
}

nav ul li {
    display: inline;
    margin-right: 15px;
}

nav a {
    color: white;
    text-decoration: none;
}

main {
    padding: 20px;
}

.car-list {
    display: flex;
    flex-wrap: wrap;
}

.car-item {
    border: 1px solid #ccc;
    margin: 10px;
    padding: 10px;
}
